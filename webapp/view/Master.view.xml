<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="app.SAPUI5MasterDetailTemplate.controller.Master" xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="Orders List">
				<List id="orders" items="{/orders}" mode="SingleSelectMaster" selectionChange="onSelectionChange">
					<items>
						<StandardListItem title="{orderName}" type="Active" press="onSelectionChange"/>
					</items>
				</List>
			</Page>
		</pages>
	</App>
</mvc:View>